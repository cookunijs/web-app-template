{
  "name": "frontend",
  "version": "0.1.0",
  "author": "cookunijs",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next && rm -rf dist",
    "dev": "cross-env APP_ENV=local nodemon",
    "deploy:dev": "yarn build && vercel",
    "deploy:prod": "yarn build && vercel --sprod",
    "build": "yarn clean && run-p build:*",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:storybook": "build-storybook -o build/storybook",
    "storybook": "start-storybook -p 8000",
    "start": "cross-env NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "firebase": "^8.1.1",
    "firebase-admin": "^9.4.1",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.30.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-docs": "^6.1.10",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storyshots": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@types/basic-auth": "^1.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-require": "^3.1.3",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "firebase-tools": "^8.17.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2",
    "vercel": "^21.0.1",
    "webpack": "^5.9.0"
  }
}
